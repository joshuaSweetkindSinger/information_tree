<!DOCTYPE html>
<html>
<head>
  <title>JssNotes</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div class="app-header">
    <a class="app-header" href=<%=root_path%>>JSS Notes</a>
  </div>
  <div class="container">
    <%= render 'shared/flash_messages' %>
    <div class="row">
      <div class = "span9 main">
        <%= yield %>
      </div>
      <div class = "span3 side-nav">
        <%= yield :side_nav %>
      </div>
    </div>
  </div>
</body>
</html>
